{
  "name": "game-engine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/game-engine/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "apps/game-engine"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/game-engine"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run type-check",
        "cwd": "apps/game-engine"
      }
    }
  },
  "tags": []
}
